"xbacklight -inc 6"
  XF86MonBrightnessUp

"xbacklight -dec 6"
  XF86MonBrightnessDown

"xbacklight -inc 1"
  Shift + alt + XF86MonBrightnessUp

"xbacklight -dec 1"
  Shift + alt + XF86MonBrightnessDown

"xbacklight -set 1"
  XF86LaunchA

"xbacklight -set 100"
  XF86LaunchB

