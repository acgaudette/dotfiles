#!/usr/bin/env bash

cat ~/bin/sgit | grep alias | sed 's/alias//' | tail -n +2

alias g='git log'
alias gg='git log --graph --oneline --all'

alias k='git checkout'
alias b='git branch'
alias m='git merge'

alias rr='git rebase -i master'
alias cc='git rebase --continue'

alias d='clear; git diff'
alias ds='d --staged'

alias a='git add'
alias r='git reset'

alias s='git status .'
alias c="git commit -m '"
alias aa='git commit --amend'
