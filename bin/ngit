#!/bin/sh

alias s='git status .'
alias d='git diff'
alias a='git add'
alias r='git reset'
alias b='git branch'
alias k='git checkout'
alias g='git log'
alias w='git show'
# ./c

alias ds='d --staged'
alias ap='a -p'
alias bb='b | grep \* | cut -c 3-'
alias kb='k -b'
alias gp='g -p'
alias gg='g --graph --oneline --all'
alias aa='git commit --amend'
alias rr='git rebase -i'
alias cc='git rebase --continue'
alias ab='git rebase --abort'
alias px='git cherry-pick -x'
alias ts="git tag -l --format='%(contents)' $1"

alias gpp='gp -1'
